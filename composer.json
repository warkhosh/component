{
  "name": "warkhosh/component",
  "type": "library",
  "description": "Набор универсальных хелперов, классов, библиотек и т.д.",
  "keywords": [
    "php",
    "basic",
    "class",
    "Helpers"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "support": {
    "email": "ekv.programmer@gmail.com"
  },
  "authors": [
    {
      "name": "Konstantin Egorov",
      "email": "ekv.programmer@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://gitverse.ru/warkhosh/assist.git"
    },
    {
      "type": "git",
      "url": "https://gitverse.ru/warkhosh/storage.git"
    },
    {
      "type": "git",
      "url": "https://gitverse.ru/warkhosh/singleton.git"
    },
    {
      "type": "git",
      "url": "https://gitverse.ru/warkhosh/exception.git"
    }
  ],
  "require": {
    "php": "^8.0 || ^8.2",
    "psr/http-client": "^1.0",
    "psr/simple-cache": "^1.0",
    "warkhosh/assist": "dev-master",
    "warkhosh/storage": "dev-master",
    "warkhosh/singleton": "dev-master",
    "warkhosh/exception": "dev-master",
    "ext-simplexml": "*",
    "ext-mbstring": "*",
    "ext-fileinfo": "*",
    "ext-iconv": "*",
    "ext-posix": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-dom": "*"

  },
  "require-dev": {
    "jetbrains/phpstorm-attributes": "dev-master",
    "laravel/pint": "^1.18"
  },
  "autoload": {
    "psr-4": {
      "Warkhosh\\Component\\": "src"
    },
    "files": [
      "src/Support/Helpers.php"
    ]
  }
}
